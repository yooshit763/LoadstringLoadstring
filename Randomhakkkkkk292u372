local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/Brineeee/DarkHakX/main/Pakyu")()

-----/
local win = lib:Window("JioaHub","JioaHub",Color3.new(0, 255, 0))
-----/

-----/
-- Tab #1
local Tab1 = win:Tab("Home")
local label = Tab1:Label("Label", properties)
label:NewLabel("|| Home ||", properties)
Tab1:Line()
-----/


-- Tab #2
local Tab2 = win:Tab("Page #1")
local label = Tab2:Label("Label", properties)
label:NewLabel("|| Ultra Farm ||", properties)
Tab2:Line()
-----/

-----/
-- Tab #3
local Tab3 = win:Tab("Page #2")
local label = Tab3:Label("Label", properties)
label:NewLabel("|| Basic Farm ||", properties)
Tab3:Line()
-----/

Tab1:Button("Credits [ Please Click ]", function()
lib:Nof("[?] For Script = mech#0945", 3)
wait(1)
lib:Nof("[?] For Library = HoHo UI", 3)
end)

Tab2:Button("Farm - XP 1000", function()
_G.Loll = true
while _G.Loll == true do wait(0)
for i=1,1000 do
local tbl_main = 
{
      "collectOrb", 
      "Red Orb", 
      "City"
}
game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer(unpack(tbl_main))

local tbl_main = 
{
      "collectOrb", 
      "Yellow Orb", 
      "City"
}
game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer(unpack(tbl_main))

local tbl_main = 
{
      "collectOrb", 
      "Gem", 
      "City"
}
game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer(unpack(tbl_main))

local tbl_main = 
{
      "collectOrb", 
      "Orange Orb", 
      "City"
}
game:GetService("ReplicatedStorage").rEvents.orbEvent:FireServer(unpack(tbl_main))

local tbl_main = 
{
      "rebirthRequest"
}
game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(unpack(tbl_main))
end
end
	end)

